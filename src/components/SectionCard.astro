---
import Card from "./Card.astro"
import {data} from "../lib/dolarapi"

interface Data {
  nombre: String;
  compra: number;
  venta: number;
  fechaActualizacion: String;
}


---


<ul class="grid grid-cols-1 md:grid-cols-3 gap-x-4 gap-y-6">

    {data.map(({nombre, compra, venta, fechaActualizacion}:Data)=>{
        return( <Card nombre={nombre} compra={compra} venta={venta} fechaActualizacion={fechaActualizacion} />)
    }) }
    
   
   
  </ul>
